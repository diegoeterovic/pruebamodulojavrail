<p id="notice"><%= notice %></p>

<h1>Bookmarks</h1>

<div id ="form_bookmark">
  <%= render  "form", bookmark: @bookmark %>
</div>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th>Category</th>
      <th>Tipo</th>
